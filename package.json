{
  "name": "@kubric/redis-cache",
  "version": "0.1.0",
  "description": "Redis cache wrapper over ioredis with configurable entities",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "rollup -w -c",
    "build": "rimraf dist && rollup -c",
    "test": "jest",
    "test-watch": "jest --watch",
    "prepare": "install-peers",
    "pack:list": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cirbuk/redis-cache.git"
  },
  "author": "Jophin Joseph",
  "contributors": [
    "Abhijith Vijayan <abhijith.v@kubric.io>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cirbuk/redis-cache/issues"
  },
  "lint-staged": {
    "*.js": [
      "eslint --max-warnings 0 --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "homepage": "https://github.com/cirbuk/redis-cache",
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-function-bind": "^7.18.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-babel": "5.1.0",
    "@rollup/plugin-commonjs": "14.0.0",
    "@rollup/plugin-node-resolve": "8.4.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/ioredis": "^4.28.10",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "babel-jest": "^29.4.1",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "install-peers-cli": "2.2.0",
    "jest": "^29.4.1",
    "lint-staged": "^13.1.0",
    "lodash": "^4.17.21",
    "prettier": "^2.8.3",
    "rimraf": "3.0.2",
    "rollup": "2.45.2",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.30.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@kubric/utils": ">=1.0.6"
  },
  "dependencies": {
    "ioredis": "^4.28.5",
    "ioredis-lock": "^4.0.0"
  }
}
